<section class="cprecap-info" id="about">
    <div class="container-xl" style="min-height:65vh">
        <div class="row justify-content-center box-bg-container">

			<div class="col-md-12">
				<h2>Beta Testers</h2>
				<% if(users && users.length > 0) { %>
					<table class="table">
						<tr>
							<th>Username</th>
							<th>Email</th>
							<th>Date Joined</th>
							<th></th>
						</tr>
						<% users.forEach(function(user,key) {  %>
							<tr>
								<td><%= user.username %></td>
								<td><%= user.email %></td>
								<td><%= moment(user.testerJoinDate).format("MMMM Do YYYY") %></td>
								<td>
									<a href="#"><i class="fa fa-pen"></i></a>
									<a href="#"><i class="fa fa-trash"></i></a>
								</td>
							</tr>
						<% }) %>
					</table>
				<% } else { %>
					<p>Sorry, no topics found!</p>
				<% } %>
			</div>

			

		
		</div>
    </div>
</section>
